  "scripts": {
    "start": "node index.js",
    "start:optimized": "node --max-old-space-size=512 --optimize-for-size index.js",
    "pm2:start": "pm2 start index.js --name Vortex-S2",
    "pm2:stop": "pm2 stop Vortex-S2",
    "pm2:restart": "pm2 restart Vortex-S2",
    "pm2:logs": "pm2 logs Vortex-S2",
    "cleanup": "node cleanup.js",
    "reset-session": "node reset-session.js",
    "start:fresh": "npm run reset-session && npm run start",
    "install:panel": "npm install",
    "docker:build": "docker build -t docker run -e SESSION_ID=$SESSION_ID vortex-s2"
  },

