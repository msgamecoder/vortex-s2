{
  "name": "Vortex-S2",
  "description": "Vortex-S2, simple Whatsapp bot by mxgamecoder!",
  "logo": "https://i.ibb.co/Q7Lv5JBk/zenitsu-agatsuma-3840x2160-24472.png",
  "keywords": [],
  "repository": "https://github.com/msgamecoder/vortex-s2",
  "stack": "container",
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ],
  "scripts": {
